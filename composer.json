{
    "name": "rockero-cz/db-updates",
    "description": "This is my package db-updates",
    "keywords": [
        "rockero-cz",
        "laravel",
        "db-updates"
    ],
    "homepage": "https://github.com/rockero-cz/db-updates",
    "license": "MIT",
    "authors": [
        {
            "name": "Rockero",
            "email": "info@rockero.cz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.13.0",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Rockero\\DatabaseUpdates\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rockero\\DatabaseUpdates\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rockero\\DatabaseUpdates\\DatabaseUpdatesServiceProvider"
            ],
            "aliases": {
                "DatabaseUpdates": "Rockero\\DatabaseUpdates\\Facades\\DatabaseUpdates"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
